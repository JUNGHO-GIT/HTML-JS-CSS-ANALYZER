{
  "name": "html-js-css-analyzer",
  "displayName": "Html-Js-Css-Analyzer",
  "description": "HTML, JavaScript, CSS analyzer and linter extension for Visual Studio Code.",
  "publisher": "JUNGHO",
  "icon": "logo.webp",
  "license": "Apache-2.0",
  "version": "1.0.64",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNGHO-GIT/HTML-JS-CSS-ANALYZER.git"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "keywords": [
    "css",
    "html",
    "javascript",
    "typescript",
    "jshint",
    "htmlhint",
    "intellisense",
    "diagnostics",
    "unused",
    "selectors"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onCommand:Html-Js-Css-Analyzer.validate",
    "onCommand:Html-Js-Css-Analyzer.clear"
  ],
  "contributes": {
    "outputChannels": [
      "Html-Js-Css-Analyzer"
    ],
    "commands": [
      {
        "command": "Html-Js-Css-Analyzer.validate",
        "title": "Html-Js-Css-Analyzer: Validate Current Document"
      },
      {
        "command": "Html-Js-Css-Analyzer.clear",
        "title": "Html-Js-Css-Analyzer: Clear Style Cache"
      }
    ],
    "configuration": {
      "title": "Html-Js-Css-Analyzer",
      "properties": {
        "Html-Js-Css-Analyzer.logLevel": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "error",
            "info",
            "debug"
          ],
          "default": "debug"
        },
        "Html-Js-Css-Analyzer.exclude": {
          "type": "array",
          "scope": "resource",
          "default": [
            "**/node_modules/**",
            "**/.git/**",
            "**/dist/**",
            "**/out/**",
            "**/.svn/**",
            "**/.hg/**",
            "**/CVS/**",
            "**/.idea/**",
            "**/.vscode/**",
            "**/.settings/**",
            "**/.metadata/**",
            "**/.history/**",
            "**/.backup/**",
            "**/.etc/**",
            "**/.cache/**",
            "**/.gradle/**",
            "**/.mvn/**",
            "**/bin/**",
            "**/build/**",
            "**/target/**",
            "**/logs/**",
            "**/.pytest_cache/**",
            "**/.scannerwork/**",
            "**/.terraform/**",
            "**/__pycache__/**",
            "**/.venv/**",
            "**/.classpath",
            "**/.project",
            "**/.factorypath",
            "**/.DS_Store",
            "**/Thumbs.db",
            "**/desktop.ini",
            "**/.coverage"
          ],
          "items": {
            "type": "string"
          }
        },
        "Html-Js-Css-Analyzer.additionalExtensions": {
          "type": "array",
          "scope": "resource",
          "default": [
            "html",
            "htm",
            "css",
            "scss"
          ],
          "items": {
            "type": "string"
          },
          "description": "[Html-Js-Css-Analyzer] Additional file extensions to analyze"
        },
        "Html-Js-Css-Analyzer.htmlHint.enabled": {
          "type": "boolean",
          "scope": "window",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable HTML analysis and hints"
        },
        "Html-Js-Css-Analyzer.cssHint.enabled": {
          "type": "boolean",
          "scope": "window",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable CSS analysis and hints"
        },
        "Html-Js-Css-Analyzer.jsHint.enabled": {
          "type": "boolean",
          "scope": "window",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable JavaScript analysis and hints"
        },
        "Html-Js-Css-Analyzer.tsHint.enabled": {
          "type": "boolean",
          "scope": "window",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable TypeScript analysis and hints"
        }
      }
    }
  },
  "scripts": {
    "watch": "node .node/swc.cjs --watch",
    "compile": "node .node/swc.cjs --compile",
    "reset": "node .node/reset.cjs --pnpm",
    "gcloud": "node .node/gcloud.cjs --git",
    "autofix": "node .node/autofix.cjs --fix",
    "vsce": "node .node/vsce.cjs"
  },
  "engines": {
    "vscode": "^1.106.0",
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@swc/helpers": "^0.5.17",
    "htmlhint": "^1.7.1",
    "jshint": "^2.13.6",
    "minimatch": "^10.1.1",
    "terser": "^5.44.1"
  },
  "devDependencies": {
    "@types/jshint": "^2.12.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/vscode": "^1.106.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.0",
    "@types/htmlhint": "^1.1.5",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.2",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "rimraf": "^6.1.0",
    "mocha": "^11.7.5",
    "ts-morph": "^27.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "ts-prune": "^0.10.3"
  }
}
