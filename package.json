{
  "name": "html-js-css-analyzer",
  "displayName": "Html-Js-Css-Analyzer",
  "description": "HTML, JavaScript, CSS analyzer and linter extension for Visual Studio Code.",
  "publisher": "JUNGHO",
  "icon": "logo.webp",
  "license": "Apache-2.0",
  "version": "2.9.9",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNGHO-GIT/HTML-JS-CSS-ANALYZER.git"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "keywords": [
    "css",
    "html",
    "javascript",
    "jshint",
    "htmlhint",
    "intellisense",
    "diagnostics",
    "unused",
    "selectors"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript",
    "onCommand:Html-Js-Css-Analyzer.validate",
    "onCommand:Html-Js-Css-Analyzer.clear"
  ],
  "contributes": {
    "outputChannels": [
      "Html-Js-Css-Analyzer"
    ],
    "commands": [
      {
        "command": "Html-Js-Css-Analyzer.validate",
        "title": "Html-Js-Css-Analyzer: Validate Current Document"
      },
      {
        "command": "Html-Js-Css-Analyzer.clear",
        "title": "Html-Js-Css-Analyzer: Clear Style Cache"
      }
    ],
    "configuration": {
      "title": "Html-Js-Css-Analyzer",
      "properties": {
        "Html-Js-Css-Analyzer.logLevel": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info"
        },
        "Html-Js-Css-Analyzer.exclude": {
          "type": "array",
          "scope": "resource",
          "default": [
            "**/node_modules/**",
            "**/.git/**",
            "**/dist/**",
            "**/out/**",
            "**/.svn/**",
            "**/.hg/**",
            "**/CVS/**",
            "**/.idea/**",
            "**/.vscode/**",
            "**/.settings/**",
            "**/.metadata/**",
            "**/.history/**",
            "**/.backup/**",
            "**/.etc/**",
            "**/.cache/**",
            "**/.gradle/**",
            "**/.mvn/**",
            "**/bin/**",
            "**/build/**",
            "**/target/**",
            "**/logs/**",
            "**/.pytest_cache/**",
            "**/.scannerwork/**",
            "**/.terraform/**",
            "**/__pycache__/**",
            "**/.venv/**",
            "**/.classpath",
            "**/.project",
            "**/.factorypath",
            "**/.DS_Store",
            "**/Thumbs.db",
            "**/desktop.ini",
            "**/.coverage"
          ],
          "items": {
            "type": "string"
          }
        },
        "Html-Js-Css-Analyzer.additionalExtensions": {
          "type": "array",
          "scope": "resource",
          "default": [
            "html",
            "htm",
            "css"
          ],
          "items": {
            "type": "string"
          },
          "description": "[Html-Js-Css-Analyzer] Additional file extensions to analyze"
        },
        "Html-Js-Css-Analyzer.htmlHint.enabled": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable HTML analysis and hints"
        },
        "Html-Js-Css-Analyzer.cssHint.enabled": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "[Html-Js-Css-Analyzer] Enable/disable CSS analysis and hints"
        }
      }
    }
  },
  "engines": {
    "vscode": ">=1.106.1",
    "node": ">=21.0.0",
    "npm": ">=10.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "start-client": "bun .node/swc.cjs --bun --start --client",
    "start-server": "bun .node/swc.cjs --bun --start --server",
    "build-client": "bun .node/swc.cjs --bun --build --client",
    "build-server": "bun .node/swc.cjs --bun --build --server",
    "git-fetch": "bun .node/git.cjs --bun --fetch",
    "git-push": "bun .node/git.cjs --bun --push",
    "fix": "bun .node/fix.cjs --bun --fix",
    "reset": "bun .node/reset.cjs --bun --reset",
    "vsce": "bun .node/vsce.cjs --bun --package",
    "gcloud": "bun .node/gcloud.cjs --bun --deploy"
  },
  "dependencies": {
    "htmlhint": "^1.8.0",
    "jshint": "^2.13.6",
    "minimatch": "^10.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/htmlhint": "^1.1.5",
    "@types/jshint": "^2.12.4",
    "@types/node": "^24.10.1",
    "@types/vscode": "^1.106.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "ts-morph": "^27.0.2",
    "ts-prune": "^0.10.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
